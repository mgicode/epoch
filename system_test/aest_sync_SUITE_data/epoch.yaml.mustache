{{#epoch_config}}
---
peers:
{{#peers}}
    - {{ext_addr}}
{{/peers}}

http:
    external:
        peer_address: {{ext_addr}}
    internal:
        listen_address: 0.0.0.0

websocket:
    internal:
        listen_address: 0.0.0.0

keys:
    dir: ./keys

chain:
    persist: true

mining:
    autostart: true
    cuckoo:
        miner:
            executable: mean16s-generic
            extra_args: ""
            node_bits: 16
{{/epoch_config}}
